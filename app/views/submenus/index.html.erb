<p id="notice"><%= notice %></p>

<h1>Listing Submenus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Restaurant</th>
      <th>Restaurantid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submenus.each do |submenu| %>
      <tr>
        <td><%= submenu.name %></td>
        <td><%= submenu.restaurant %></td>
        <td><%= submenu.restaurantId %></td>
        <td><%= link_to 'Show', submenu %></td>
        <td><%= link_to 'Edit', edit_submenu_path(submenu) %></td>
        <td><%= link_to 'Destroy', submenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submenu', new_submenu_path %>
